<table>
  <tr>
    <th>Name</th>
    <th>Class style</th>
    <th>Style sheets</th>
    <th>Profile</th>
    <th>Created by</th>
    <th>Updated by</th>
    <th>File location</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @data_sheets.each do |data_sheet| %>
  <tr>
    <td><%= link_to data_sheet.name, draw_globe_profile_data_sheet_path(@profile.globe, @profile, data_sheet) %></td>
    <td><%= data_sheet.class_style %></td>
    <td><%= data_sheet.style_sheets %></td>
    <td><%= link_to data_sheet.profile.name, globe_profile_path(data_sheet.profile.globe, data_sheet.profile) %></td>
    <td><%= data_sheet.creator.username %></td>
    <td><%= data_sheet.updater.username %></td>
    <td><%= data_sheet.file_location %></td>
    <td><%= link_to 'Edit', edit_globe_profile_data_sheet_path(@profile.globe, @profile, data_sheet) %></td>
    <td><%= link_to 'Destroy', [data_sheet.profile.globe, data_sheet.profile, data_sheet], :confirm => 'Are you sure?', :method => :delete %></td>
    <td><%= link_to 'Render', draw_globe_profile_data_sheet_path(@profile.globe, @profile, data_sheet) %></td>
  </tr>
<% end %>
</table>

<%= render :partial => 'data_sheets/list_nav', :locals => { :show_new => true }%>
