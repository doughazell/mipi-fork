<h1>All Data Line Items</h1>
<table>
  <th>Name</th>
  <th>Short Name</th>
  <th>Description</th>
  <th>Data Set</th>
  <% @line_item_array.each do |line_item| %>
  <tr>
    <td><%= LinkDataSheet(line_item, "name", {:data_sheet => line_item.name}) %></td>
    <td><%= line_item.short_name %></td>
    <td><%= line_item.description %></td>
    <td><%= line_item.data_set_data_element.friendly_name %></td>
  </tr>
  <% end %>
</table>